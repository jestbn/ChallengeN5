{
  "ApplicationName": "Web.Api",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Database": ""
  },
  "DatabaseOptions": {
    "MaxRetryCount": 3,
    "CommandTimeout": 30,
    "EnableDetailedError": true,
    "EnableSensitiveDataLogging": true
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  }
}
