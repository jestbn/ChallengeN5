{
    "AllowedHosts": "*",
    "ApplicationName": "Web.Api",
    "ConnectionStrings": {
        "Database": ""
    },
    "DatabaseOptions": {
        "MaxRetryCount": 3,
        "CommandTimeout": 30,
        "EnableDetailedError": true,
        "EnableSensitiveDataLogging": true
    },
    "ElasticConfiguration": {
        "Uri": "http://localhost:9200"
    },
    "KafkaConfiguration": "localhost:9092",
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Information",
                "System": "Warning"
            }
        }
    }
}
